<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
  <!-- 进度条背景 -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#ffffff" />
            <stroke android:width="0.0dip" android:color="#ffffff" android:dashWidth="0.0dip" android:dashGap="0.0dip" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
            <padding android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip" />
        </shape>
    </item>
    <!-- 下载歌曲时的进度 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#8dc1d0" />
                <stroke android:width="0.0dip" android:color="#ffffff" android:dashWidth="0.0dip" android:dashGap="0.0dip" />
                <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
                <padding android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip" />
            </shape>
        </clip>
    </item>
    <!-- 播放歌曲时的进度 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#69D742" />
                <stroke android:width="0.0dip" android:color="#ffffff" android:dashWidth="0.0dip" android:dashGap="0.0dip" />
                <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
                <padding android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>